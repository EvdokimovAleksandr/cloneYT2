{
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "singleAttributePerLine": true,
    "proseWrap": "always",
    "bracketSpacing": true,
    "importOrder": [
     "<THIRD_PARTY_MODULES>",
     "^@/components/(.*)$",
     "^@/ui/(.*)$",
     "^@/constants/(.*)$",
     "^@/config/(.*)$",
     "^@/hooks/(.*)$",
     "^@/utils/(.*)$",
     "^@/api/(.*)$",
     "^@/server/(.*)$",
     "^(?!.*.scss)(../.*)$",
     "^(?!.*.scss)(./.*)$",
     ".*\\.scss$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}